<div class="row">
  <div class="col-sm-22 col-lg-18 mx-auto">
    <div class="prj-TimeLine">
      <h3 class="prj-TimeLine_Title">
        タイムライン
      </h3>
      <div class="prj-TimeLine_Body">
        <% [23,22,21].each do |d| %>
          <div class="tl-Day">
            <div class="tl-Day_Head">
              <div class="tl-Date">
                <span class="tl-Date_Month">
                  10月
                </span>
                <div class="tl-Date_Day">
                  <span class="tl-Date_DayNumber">
                    <%= d %>
                  </span>
                  <span class="tl-Date_DayName">
                    水曜日
                  </span>
                </div>
              </div>
            </div>
            <div class="tl-Day_Body">
              <% 2.times do %>
                <div class="tl-ItemGroup">
                  <div class="tl-ItemGroup_Header">
                    <%= link_to 'ABC Project', '', class: 'tl-ItemGroup_Title' %>
                  </div>
                  <div class="tl-ItemGroup_Body">
                    <% 5.times do %>
                      <% time = [('09'..'20').to_a.sample, ('00'..'59').to_a.sample].join(':') %>
                      <div class="tl-Item">
                        <div class="usr-Avatar-sm usr-Avatar-red">
                          HS
                        </div>
                        <span class="tl-Item_Actor">
                          末吉 哉が
                        </span>
                        <%= link_to '管理画面の操作ログ', '', class: 'tl-Item_Link' %>
                        にコメントしました
                        <span class="tl-Item_Time">
                          <%= time %>
                        </span>
                      </div>
                    <% end -%>
                  </div>
                </div>
              <% end -%>
            </div>
          </div>
        <% end -%>
      </div>
    </div>
  </div>
</div>
