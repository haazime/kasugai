<% if @result.succeeded? -%>
  Turbolinks.clearCache();
  Turbolinks.visit("<%= project_issue_url(current_project, current_issue) %>", { action: 'replace' });
<% else -%>
  $('#js-issue-title-editor').replaceWith("<%=j render partial: 'title', locals: { issue: @issue, form: @form, open: true } %>");
<% end -%>
