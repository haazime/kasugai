<div id="<%= app_dom_id(summary) %>" class="issl-Item" data-sort-url="<%= issue_priority_url(summary) %>">
  <i class="issl-Item_Handle js-sortable-handle"></i>
  <div class="issl-Item_Body">
    <%= link_to summary.title, project_issue_url(current_project, summary), class: 'issl-Item_Content app_issue_title' %>
    <div class="issl-Item_Info">
      <%= render partial: 'comment_count', locals: { issue: summary } %>
      <%= render partial: 'bookmark', locals: { issue: summary } %>
    </div>
  </div>
  <div class="issl-Item_Tail">
    <ul class="sw-LinkList">
      <li>
        <%= link_to t('navs.edit'), edit_project_issue_url(current_project, summary), class: 'sw-Link' %>
      </li>
      <li>
        <%= link_to t('navs.delete'), project_issue_url(current_project, summary), method: :delete, class: 'sw-Link', data: { confirm: t('.confirm_delete', title: summary.title) } %>
      </li>
    </ul>
  </div>
</div>
