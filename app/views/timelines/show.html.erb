<% @dailies.each do |daily| -%>
  <div>
    <% daily.each do |project| -%>
      <div id="<%= app_dom_id(project) %>">
        <% project.each do |a| %>
          <div>
            <span>
              <%= a.project_name %>
            </span>
            <span>
              <%= a.user_name %>
            </span>
            <span>
              <%= t(a.activity, scope: 'activities.projects') %>
            </span>
            <span>
              <%= link_to a.link_text, a.link_url %>
            </span>
            <span>
              <%= a.created_at %>
            </span>
          </div>
        <% end -%>
      </div>
    <% end -%>
  </div>
<% end -%>
