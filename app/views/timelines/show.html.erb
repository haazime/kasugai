<% @dailies.each do |daily| -%>
  <div>
    <% daily.each do |project| -%>
      <div>
        <% project.each do |a| %>
          <div id="<%= app_dom_id(a) %>" class="app_activity">
            <span>
              <%= a.project_name %>
            </span>
            <span>
              <%= a.user_name %>
            </span>
            <span>
              <%= t(a.activity, scope: 'activities.projects') %>
            </span>
            <span>
              <%= a.link_to_target(self) %>
            </span>
            <span>
              <%= a.created_at %>
            </span>
          </div>
        <% end -%>
      </div>
    <% end -%>
  </div>
<% end -%>
